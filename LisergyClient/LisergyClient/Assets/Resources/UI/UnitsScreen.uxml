                        </ui:VisualElement>
                        <ui:VisualElement name="SpeedStat" class="stat-bar">
                            <ui:VisualElement name="StatIcon" class="stat-icon" style="background-image: resource(&apos;sprites/stat_spd&apos;);" />
                            <ui:Label tabindex="-1" text="999" display-tooltip-when-elided="true" name="StatValue" class="stat-text" />
                        </ui:VisualElement>
                        <ui:VisualElement name="AccuracyStat" class="stat-bar">
