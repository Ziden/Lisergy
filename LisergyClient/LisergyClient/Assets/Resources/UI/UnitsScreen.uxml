<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/Resources/UI/unitui.uss?fileID=7433441132597879392&amp;guid=b3a333282cf72e346b245018eba3482a&amp;type=3#unitui" />
    <ui:VisualElement name="Root" style="position: relative; width: 100%; height: 100%;">
        <ui:VisualElement name="UnitInfo" style="width: 359px; height: 147px; background-image: url(&apos;project://database/Assets/Addressables/UISet/panel_beige.png?fileID=21300000&amp;guid=a19b509550301614b9f2965f57308cd9&amp;type=3#panel_beige&apos;); position: absolute; right: 0; top: 0; flex-direction: column; align-items: flex-start;">
            <ui:Label tabindex="-1" text="UnitName" display-tooltip-when-elided="true" name="UnitName" style="color: rgb(0, 0, 0); -unity-font-style: bold; -unity-font: url(&apos;project://database/Assets/Packs/Thaleah_PixelFont/ThaleahFat.fontsettings?fileID=12800000&amp;guid=652f7165b7c30ab46af6c4aad90a639b&amp;type=2#ThaleahFat&apos;); font-size: 15px; -unity-text-align: upper-left; margin-left: 10px; margin-top: 5px;" />
