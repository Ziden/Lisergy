                <ui:VisualElement name="UnitPortrait" style="background-image: none; width: 60px; height: 77px; padding-left: 5px; margin-left: 3px; justify-content: center; align-items: center;" />
                <ui:VisualElement name="InfoContainer" style="width: 300px;">
                    <ui:VisualElement name="Vitals">
                        <ui:VisualElement name="VitalsContainer" style="height: 26px; align-items: flex-start; justify-content: center; flex-direction: row; padding-right: 5px;">
                            <ui:VisualElement name="VitalHP" class="vitals-bar">
                                <ui:VisualElement name="StatHP" style="background-image: resource(&apos;sprites/stat_hp&apos;); width: 29px; height: 29px; position: relative; top: 6px; left: 2px; flex-direction: column; margin-top: -10px;" />
                                <ui:ProgressBar usage-hints="None" name="HpProgress" style="background-color: rgb(19, 16, 16); height: 8px; width: 80%; max-height: 8px;" />
                            </ui:VisualElement>
                            <ui:VisualElement name="VitalMP" class="vitals-bar">
                                <ui:VisualElement name="StatMP" style="background-image: resource(&apos;sprites/stat_mp&apos;); width: 29px; height: 29px; position: relative; top: 6px; left: 2px; flex-direction: column; margin-top: -10px;" />
                                <ui:ProgressBar usage-hints="None" name="MPProgress" style="background-color: rgb(19, 16, 16); height: 8px; width: 80%;" />
                            </ui:VisualElement>
                        </ui:VisualElement>
                    </ui:VisualElement>
                    <ui:VisualElement name="StatsContainer" class="stat-container" style="flex-wrap: wrap;">
                        <ui:VisualElement name="AtkStat" class="stat-bar">
